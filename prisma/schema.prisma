// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Car {
  id         Int      @id @default(autoincrement())
  brand      String
  model      String
  price      Int
  year       Int
  kilometers Int
  fuel       String
  gearbox    String
  horsePower Int
  seats      Int?
  doors      Int?
  color      String?
  imgUrls    String[]
}
